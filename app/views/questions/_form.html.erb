<%= render 'layouts/errors', :object => @question %>
<div id="question_form">
<%= form_for [@question], remote: true do |f| %>
  <div class="form-group">
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>
  <div class="form-group">
    <%= f.label :text %>
    <%= f.text_field :text %>
  </div>
  <p><%= f.submit :class => "btn btn-primary" %></p>
<% end %>
</div>
